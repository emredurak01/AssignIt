<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import io.github.palexdev.materialfx.controls.MFXTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17"
            fx:controller="edu.ieu.assignit.Controllers.ResultsController">
    <children>
        <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox prefHeight="40.0" prefWidth="600.0">
                    <children>
                        <MFXButton id="custom" fx:id="backButton" buttonType="RAISED" prefHeight="30.0" prefWidth="65.0"
                                   styleClass="custom-button" stylesheets="@../css/buttons.css" text="&lt;"/>
                        <Pane prefHeight="26.0" prefWidth="390.0" HBox.hgrow="ALWAYS"/>
                        <MFXButton id="custom" fx:id="recompileButton" alignment="CENTER" buttonType="RAISED"
                                   prefHeight="30.0" prefWidth="89.0" styleClass="custom-button"
                                   stylesheets="@../css/buttons.css" text="Recompile"/>
                        <MFXButton id="custom" fx:id="detailsButton" alignment="CENTER" buttonType="RAISED"
                                   prefHeight="30.0" prefWidth="63.0" styleClass="custom-button"
                                   stylesheets="@../css/buttons.css" text="Details"/>
                    </children>
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                </HBox>
                <AnchorPane prefHeight="400.0" prefWidth="600.0">
                    <children>


                        <MFXTableView fx:id="table" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                    </children>
                </AnchorPane>

            </children>
        </VBox>
    </children>
</AnchorPane>
